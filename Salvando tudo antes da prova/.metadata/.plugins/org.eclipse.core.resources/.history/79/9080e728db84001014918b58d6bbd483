package br.com.byd.veiculo;

public class Moto extends Veiculo{

	private int QtddeRodas;
	private int QtdAcetnto;
	private String quidao;
	
	public Moto(int ano, String cor, String modelo, int qtddeRodas, int qtdAcetnto, String quidao) {
		super(ano, cor, modelo);
		this.QtddeRodas = qtddeRodas;
		this.QtdAcetnto = qtdAcetnto;
		this.quidao = quidao;
	}
	
	public void dargrau() {
		System.out.println("dar grau de moto hehehe...");
		
	}
	
	
}
