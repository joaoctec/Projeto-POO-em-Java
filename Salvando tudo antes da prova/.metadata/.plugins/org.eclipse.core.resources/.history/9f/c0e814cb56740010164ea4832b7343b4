package com.br.escola.principal;

public class passoui {
	public static void main(String[] args) {
		
		String nome = "joao";
        int nota = 7;
        
        if(nota > 6) {
        	System.out.println(nome+"va passou parabens");
        	
        }else {
        	System.out.println(nome+"va nao passou se fudeu");
        
        }
        	
      
	
}
}
